#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char s[100];
    char ch;
    char sen[100];
    
    scanf("%c",&ch);
    getchar();
    fgets(s,100, stdin);
    fgets(sen,100, stdin);
    
    printf("%c \n", ch );
    printf("%s", s);
    printf("%s \n", sen);
    return 0;
}
